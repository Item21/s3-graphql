    "@uppy/core": "^2.1.4",
    "@uppy/dashboard": "^2.1.3",
    "@uppy/drag-drop": "^2.0.6",
    "@uppy/progress-bar": "^2.0.4",
    "@uppy/react": "^2.1.2",
    "apollo-server-lambda": "^3.6.2",
    "aws-sdk": "^2.1070.0",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
