    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "encoding": "^0.1.13",
    "netlify-cli": "^8.18.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
